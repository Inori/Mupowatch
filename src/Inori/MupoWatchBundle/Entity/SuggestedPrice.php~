<?php
namespace Inori\GasPricesBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="suggested_price")
 */
class SuggestedPrice
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;
    
    /**
     * @ORM\Column(type="date")
     */
    protected $date;
    
    /**
     * @ORM\ManyToOne(targetEntity="Station")
     * @ORM\JoinColumn(name="station_id", referencedColumnName="id")
     */
    protected $station;
    
    /**
     * @ORM\Column(type="decimal", name="price_95")
     */
    protected $price95;   
    
    /**
     * @ORM\Column(type="decimal", name="price_98")
     */
    protected $price98;    
    
    /**
     * @ORM\Column(type="decimal", name="price_d")
     */
    protected $priceD;
    
    /**
     * @ORM\ManyToOne(targetEntity="SuggestingUser", inversedBy="prices")
     * @ORM\JoinColumn(name="user_id", referencedColumnName="id")
     */
    protected $user;
}